mob
	proc
		Levelup()
			if(src.exp>=src.max_exp)
				src.level+=1
				src.exp = 0
				src.max_exp+=rand(6500,7500)
				src.Skills()
				src.makeJounin()
				src.makeHunter()
				src<<"<font size = 2><b><font color = blue>You leveled up!!"
				if(usr.Mtai < usr.cap)
					src.taiexp+=rand(650,750)
					src.Tai()
					src.tai=src.Mtai
				else
					usr<<"Maximum Taijutsu Reached"
				if(usr.Mnin < usr.cap)
					src.ninexp+=rand(650,750)
					src.Nin()
					src.nin=src.Mnin
				else
					usr<<"Maximum Ninjutsu Reached"
				if(usr.Mgen < usr.cap)
					src.genexp+=rand(650,750)
					src.gen=src.Mgen
					src.Gen()
				else
					usr<<"Maximum Genjutsu Reached"
				if(src.Mhealth < src.Stamcap)
					src.Mhealth+=rand(500,1000)
				else
					usr<<"Maximum Stamina Reached"
				if(src.Mchakra < src.Chakracap)
					src.Mchakra+=rand(500,1000)
				else
					usr<<"Maximum Chakra Reached"
		Tai()
			if(usr.Mtai < usr.cap)
				if(src.taiexp>=src.mtaiexp)
					src.Skills()
					src.makeJounin()
					src.makeHunter()
					src.mtaiexp += rand(950,1000)
					src.tailvl+=1
					src.taiexp = 0
					src.level+=1
					src.Mtai+=rand(950,1200)
					src.tai=src.Mtai
					src<<"<font size = 2><b><font color = blue>You've Leveled Up Your Taijutsu"
			else
				usr<<"Maximum Taijutsu Reached"
		Nin()
			if(usr.Mnin < usr.cap)
				if(src.ninexp>=src.mninexp)
					src.Skills()
					src.makeJounin()
					src.makeHunter()
					src.mninexp += rand(950,1000)
					src.ninexp = 0
					src.Mnin+=rand(950,1000)
					src.level+=1
					src.nin=src.Mnin
					src<<"<font size = 2><b><font color = blue>You've Leveled Up Your Ninjutsu"
			else
				usr<<"Maximum Ninjutsu Reached"
		Gen()
			if(usr.Mgen < usr.cap)
				if(src.genexp>=src.mgenexp)
					src.Skills()
					src.makeJounin()
					src.makeHunter()
					src.mgenexp += rand(950,1000)
					src.level+=1
					src.genexp = 0
					src.Mgen+=rand(950,1000)
					src.gen=src.Mgen
					src<<"<font size = 2><b><font color = blue>You've Leveled Up Your Genjutsu"
			else
				usr<<"Maximum Genjutsu Reached"
